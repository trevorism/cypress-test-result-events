{
  "name": "@trevorism/cypress-test-result-events",
  "version": "0.2.0",
  "description": "Cypress plugin to send a test result event",
  "type": "commonjs",
  "scripts": {
    "build": "vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "accept": "vitest run --environment jsdom",
    "dev": "vite",
    "preview": "vite preview --port 4173",
    "format": "prettier --write ./*.js",
    "test": "vitest --environment jsdom",
    "webstorm-integration": "vitest --watch --reporter=dot --reporter=json --outputFile=.vitest-result.json"
  },
  "main": "./dist/cypress-test-result-events.umd.js",
  "module": "./dist/cypress-test-result-events.es.js",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.1",
    "@types/node": "^20.10.5",
    "eslint": "^8.56.0",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.1",
    "resize-observer-polyfill": "^1.5.1",
    "vite": "^5.0.10",
    "vitest": "^1.6.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trevorism/cypress-test-result-events.git"
  },
  "dependencies": {
    "axios": "^1.7.2"
  }
}
